{
  "LaunchTemplateName": "g6e-xlarge-deeplearning-template",
  "VersionDescription": "g6e.xlarge instance with AWS Deep Learning Base GPU AMI (Ubuntu 22.04)",
  "LaunchTemplateData": {
    "InstanceType": "g6e.xlarge",
    "ImageId": "ami-07e05a9b31a088ae5",
    "KeyName": "YOUR-KEY-NAME",
    "BlockDeviceMappings": [
      {
        "DeviceName": "/dev/sda1",
        "Ebs": {
          "VolumeSize": 100,
          "VolumeType": "gp3",
          "DeleteOnTermination": true,
          "Iops": 3000,
          "Throughput": 125
        }
      }
    ],
    "NetworkInterfaces": [
      {
        "AssociatePublicIpAddress": true,
        "DeleteOnTermination": true,
        "DeviceIndex": 0,
        "Groups": ["YOUR-SECURITY-GROUP-ID"]
      }
    ],
    "TagSpecifications": [
      {
        "ResourceType": "instance",
        "Tags": [
          {
            "Key": "Name",
            "Value": "g6e-xlarge-instance"
          },
          {
          "Key": "OS",
          "Value": "Deep-Learning-AMI-Ubuntu-22.04"
          }
        ]
      },
      {
        "ResourceType": "volume",
        "Tags": [
          {
            "Key": "Name",
            "Value": "g6e-xlarge-volume"
          }
        ]
      }
    ],
    "UserData": "",
    "MetadataOptions": {
      "HttpTokens": "optional",
      "HttpPutResponseHopLimit": 2,
      "HttpEndpoint": "enabled"
    }
  }
}
